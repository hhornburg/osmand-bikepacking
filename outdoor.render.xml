<renderingStyle name="Outdoor" depends="" defaultColor="#ffffff" version="1">
    <!-- 
    Concept of the paths:
    (indices apply also to other attributes, not only color)
    color__1: route background marking
    color_0: path outline (i.e. track)
    color: path fill
    color_2: difficulty markup key (white box)
    color_3: difficulty markup mtb scale
    color_4: difficulty markup mtb uphill scale
    color_5: difficulty markup sac scale
    -->

    <renderingConstant name="width_l1_z12" value="1"/>
    <renderingConstant name="width_l1_z13" value="2"/>
    <renderingConstant name="width_l1_z14" value="3"/>
    <renderingConstant name="width_l1_z15" value="4"/>
    <renderingConstant name="width_l1_z16" value="5"/>
    <renderingConstant name="width_l1_z17" value="6"/>
    <renderingConstant name="width_l1_z18" value="7"/>
    <renderingConstant name="width_l1_z19" value="8"/>
    <renderingConstant name="width_l1_z20" value="9"/>
    <renderingConstant name="width_l1_z21" value="10"/>

    <renderingConstant name="width_l2_z12" value="2"/>
    <renderingConstant name="width_l2_z13" value="4"/>
    <renderingConstant name="width_l2_z14" value="6"/>
    <renderingConstant name="width_l2_z15" value="8"/>
    <renderingConstant name="width_l2_z16" value="10"/>
    <renderingConstant name="width_l2_z17" value="12"/>
    <renderingConstant name="width_l2_z18" value="14"/>
    <renderingConstant name="width_l2_z19" value="16"/>
    <renderingConstant name="width_l2_z20" value="18"/>
    <renderingConstant name="width_l2_z21" value="20"/>

    <renderingAttribute name="width_l1">
        <case maxzoom="12" strokeWidth="$width_l1_z12" />
        <case minzoom="13" maxzoom="13" strokeWidth="$width_l1_z13"/>
        <case minzoom="14" maxzoom="14" strokeWidth="$width_l1_z14"/>
        <case minzoom="15" maxzoom="15" strokeWidth="$width_l1_z15" strokeWidth_2="$width_l1_z15" strokeWidth_3="$width_l1_z15" strokeWidth_4="$width_l1_z15" strokeWidth_5="$width_l1_z15"/>
        <case minzoom="16" maxzoom="16" strokeWidth="$width_l1_z16" strokeWidth_2="$width_l1_z16" strokeWidth_3="$width_l1_z16" strokeWidth_4="$width_l1_z16" strokeWidth_5="$width_l1_z16"/>
        <case minzoom="17" maxzoom="17" strokeWidth="$width_l1_z17" strokeWidth_2="$width_l1_z17" strokeWidth_3="$width_l1_z17" strokeWidth_4="$width_l1_z17" strokeWidth_5="$width_l1_z17"/>
        <case minzoom="18" maxzoom="18" strokeWidth="$width_l1_z18" strokeWidth_2="$width_l1_z18" strokeWidth_3="$width_l1_z18" strokeWidth_4="$width_l1_z18" strokeWidth_5="$width_l1_z18"/>
        <case minzoom="19" maxzoom="19" strokeWidth="$width_l1_z19" strokeWidth_2="$width_l1_z19" strokeWidth_3="$width_l1_z19" strokeWidth_4="$width_l1_z19" strokeWidth_5="$width_l1_z19"/>
        <case minzoom="20" maxzoom="20" strokeWidth="$width_l1_z20" strokeWidth_2="$width_l1_z20" strokeWidth_3="$width_l1_z20" strokeWidth_4="$width_l1_z20" strokeWidth_5="$width_l1_z20"/>
        <case minzoom="21" strokeWidth="$width_l1_z21" strokeWidth_2="$width_l1_z21" strokeWidth_3="$width_l1_z21" strokeWidth_4="$width_l1_z21" strokeWidth_5="$width_l1_z21"/>
    </renderingAttribute>

    <renderingAttribute name="width_l2">
        <case maxzoom="12" strokeWidth_0="$width_l2_z12" />
        <case minzoom="13" maxzoom="13" strokeWidth_0="$width_l2_z13" />
        <case minzoom="14" maxzoom="14" strokeWidth_0="$width_l2_z14" />
        <case minzoom="15" maxzoom="15" strokeWidth_0="$width_l2_z15" />
        <case minzoom="16" maxzoom="16" strokeWidth_0="$width_l2_z16" />
        <case minzoom="17" maxzoom="17" strokeWidth_0="$width_l2_z17" />
        <case minzoom="18" maxzoom="18" strokeWidth_0="$width_l2_z18" />
        <case minzoom="19" maxzoom="19" strokeWidth_0="$width_l2_z19" />
        <case minzoom="20" maxzoom="20" strokeWidth_0="$width_l2_z20" />
        <case minzoom="21" strokeWidth_0="$width_l2_z21" />
    </renderingAttribute>

    <!-- unknown=horrible+good-->
    <renderingConstant name="pathEffect_unknown_z12" value="4_5_7_2"/>
    <renderingConstant name="pathEffect_unknown_z13" value="8_10_14_4"/>
    <renderingConstant name="pathEffect_unknown_z14" value="12_15_21_6"/>
    <renderingConstant name="pathEffect_unknown_z15" value="16_20_28_8"/>
    <renderingConstant name="pathEffect_unknown_z16" value="20_25_35_10"/>
    <renderingConstant name="pathEffect_unknown_z17" value="24_30_42_12"/>
    <renderingConstant name="pathEffect_unknown_z18" value="28_35_49_14"/>
    <renderingConstant name="pathEffect_unknown_z19" value="32_40_56_16"/>
    <renderingConstant name="pathEffect_unknown_z20" value="34_45_63_18"/>
    <renderingConstant name="pathEffect_unknown_z21" value="38_50_70_20"/>

    <renderingConstant name="pathEffect_good_z12" value="7_2"/>
    <renderingConstant name="pathEffect_good_z13" value="14_4"/>
    <renderingConstant name="pathEffect_good_z14" value="21_6"/>
    <renderingConstant name="pathEffect_good_z15" value="28_8"/>
    <renderingConstant name="pathEffect_good_z16" value="35_10"/>
    <renderingConstant name="pathEffect_good_z17" value="42_12"/>
    <renderingConstant name="pathEffect_good_z18" value="49_14"/>
    <renderingConstant name="pathEffect_good_z19" value="56_16"/>
    <renderingConstant name="pathEffect_good_z20" value="63_18"/>
    <renderingConstant name="pathEffect_good_z21" value="70_20"/>

    <renderingConstant name="pathEffect_intermediate_z12" value="6_3"/>
    <renderingConstant name="pathEffect_intermediate_z13" value="12_6"/>
    <renderingConstant name="pathEffect_intermediate_z14" value="18_9"/>
    <renderingConstant name="pathEffect_intermediate_z15" value="24_12"/>
    <renderingConstant name="pathEffect_intermediate_z16" value="30_15"/>
    <renderingConstant name="pathEffect_intermediate_z17" value="36_18"/>
    <renderingConstant name="pathEffect_intermediate_z18" value="42_21"/>
    <renderingConstant name="pathEffect_intermediate_z19" value="48_24"/>
    <renderingConstant name="pathEffect_intermediate_z20" value="54_27"/>
    <renderingConstant name="pathEffect_intermediate_z21" value="60_30"/>

    <renderingConstant name="pathEffect_bad_z12" value="5_4"/>
    <renderingConstant name="pathEffect_bad_z13" value="10_8"/>
    <renderingConstant name="pathEffect_bad_z14" value="15_12"/>
    <renderingConstant name="pathEffect_bad_z15" value="20_16"/>
    <renderingConstant name="pathEffect_bad_z16" value="25_20"/>
    <renderingConstant name="pathEffect_bad_z17" value="30_24"/>
    <renderingConstant name="pathEffect_bad_z18" value="35_28"/>
    <renderingConstant name="pathEffect_bad_z19" value="40_32"/>
    <renderingConstant name="pathEffect_bad_z20" value="45_36"/>
    <renderingConstant name="pathEffect_bad_z21" value="50_40"/>

    <renderingConstant name="pathEffect_horrible_z12" value="4_5"/>
    <renderingConstant name="pathEffect_horrible_z13" value="8_10"/>
    <renderingConstant name="pathEffect_horrible_z14" value="12_15"/>
    <renderingConstant name="pathEffect_horrible_z15" value="16_20"/>
    <renderingConstant name="pathEffect_horrible_z16" value="20_25"/>
    <renderingConstant name="pathEffect_horrible_z17" value="24_30"/>
    <renderingConstant name="pathEffect_horrible_z18" value="28_35"/>
    <renderingConstant name="pathEffect_horrible_z19" value="32_40"/>
    <renderingConstant name="pathEffect_horrible_z20" value="36_45"/>
    <renderingConstant name="pathEffect_horrible_z21" value="40_50"/>

    <renderingConstant name="pathEffect_no_z12" value="3_6"/>
    <renderingConstant name="pathEffect_no_z13" value="6_12"/>
    <renderingConstant name="pathEffect_no_z14" value="9_18"/>
    <renderingConstant name="pathEffect_no_z15" value="12_24"/>
    <renderingConstant name="pathEffect_no_z16" value="15_30"/>
    <renderingConstant name="pathEffect_no_z17" value="18_36"/>
    <renderingConstant name="pathEffect_no_z18" value="21_42"/>
    <renderingConstant name="pathEffect_no_z19" value="24_48"/>
    <renderingConstant name="pathEffect_no_z20" value="27_54"/>
    <renderingConstant name="pathEffect_no_z21" value="30_60"/>

    <renderingAttribute name="pathEffect_unknown">
        <case maxzoom="12" pathEffect_0="$pathEffect_unknown_z12" pathEffect="$pathEffect_unknown_z12" />
        <case minzoom="13" maxzoom="13" pathEffect_0="$pathEffect_unknown_z13" pathEffect="$pathEffect_unknown_z13" />
        <case minzoom="14" maxzoom="14" pathEffect_0="$pathEffect_unknown_z14" pathEffect="$pathEffect_unknown_z14" />
        <case minzoom="15" maxzoom="15" pathEffect_0="$pathEffect_unknown_z15" pathEffect="$pathEffect_unknown_z15" />
        <case minzoom="16" maxzoom="16" pathEffect_0="$pathEffect_unknown_z16" pathEffect="$pathEffect_unknown_z16" />
        <case minzoom="17" maxzoom="17" pathEffect_0="$pathEffect_unknown_z17" pathEffect="$pathEffect_unknown_z17" />
        <case minzoom="18" maxzoom="18" pathEffect_0="$pathEffect_unknown_z18" pathEffect="$pathEffect_unknown_z18" />
        <case minzoom="19" maxzoom="19" pathEffect_0="$pathEffect_unknown_z19" pathEffect="$pathEffect_unknown_z19" />
        <case minzoom="20" maxzoom="20" pathEffect_0="$pathEffect_unknown_z20" pathEffect="$pathEffect_unknown_z20" />
        <case minzoom="21" pathEffect_0="$pathEffect_unknown_z21" pathEffect="$pathEffect_unknown_z21" />
    </renderingAttribute>

    <renderingAttribute name="pathEffect_good">
        <case maxzoom="12" pathEffect_0="$pathEffect_good_z12" pathEffect="$pathEffect_good_z12" />
        <case minzoom="13" maxzoom="13" pathEffect_0="$pathEffect_good_z13" pathEffect="$pathEffect_good_z13" />
        <case minzoom="14" maxzoom="14" pathEffect_0="$pathEffect_good_z14" pathEffect="$pathEffect_good_z14" />
        <case minzoom="15" maxzoom="15" pathEffect_0="$pathEffect_good_z15" pathEffect="$pathEffect_good_z15" />
        <case minzoom="16" maxzoom="16" pathEffect_0="$pathEffect_good_z16" pathEffect="$pathEffect_good_z16" />
        <case minzoom="17" maxzoom="17" pathEffect_0="$pathEffect_good_z17" pathEffect="$pathEffect_good_z17" />
        <case minzoom="18" maxzoom="18" pathEffect_0="$pathEffect_good_z18" pathEffect="$pathEffect_good_z18" />
        <case minzoom="19" maxzoom="19" pathEffect_0="$pathEffect_good_z19" pathEffect="$pathEffect_good_z19" />
        <case minzoom="20" maxzoom="20" pathEffect_0="$pathEffect_good_z20" pathEffect="$pathEffect_good_z20" />
        <case minzoom="21" pathEffect_0="$pathEffect_good_z21" pathEffect="$pathEffect_good_z21" />
    </renderingAttribute>

    <renderingAttribute name="pathEffect_intermediate">
        <case maxzoom="12" pathEffect_0="$pathEffect_intermediate_z12" pathEffect="$pathEffect_intermediate_z12" />
        <case minzoom="13" maxzoom="13" pathEffect_0="$pathEffect_intermediate_z13" pathEffect="$pathEffect_intermediate_z13" />
        <case minzoom="14" maxzoom="14" pathEffect_0="$pathEffect_intermediate_z14" pathEffect="$pathEffect_intermediate_z14" />
        <case minzoom="15" maxzoom="15" pathEffect_0="$pathEffect_intermediate_z15" pathEffect="$pathEffect_intermediate_z15" />
        <case minzoom="16" maxzoom="16" pathEffect_0="$pathEffect_intermediate_z16" pathEffect="$pathEffect_intermediate_z16" />
        <case minzoom="17" maxzoom="17" pathEffect_0="$pathEffect_intermediate_z17" pathEffect="$pathEffect_intermediate_z17" />
        <case minzoom="18" maxzoom="18" pathEffect_0="$pathEffect_intermediate_z18" pathEffect="$pathEffect_intermediate_z18" />
        <case minzoom="19" maxzoom="19" pathEffect_0="$pathEffect_intermediate_z19" pathEffect="$pathEffect_intermediate_z19" />
        <case minzoom="20" maxzoom="20" pathEffect_0="$pathEffect_intermediate_z20" pathEffect="$pathEffect_intermediate_z20" />
        <case minzoom="21" pathEffect_0="$pathEffect_intermediate_z21" pathEffect="$pathEffect_intermediate_z21" />
    </renderingAttribute>

    <renderingAttribute name="pathEffect_bad">
        <case maxzoom="12" pathEffect_0="$pathEffect_bad_z12" pathEffect="$pathEffect_bad_z12" />
        <case minzoom="13" maxzoom="13" pathEffect_0="$pathEffect_bad_z13" pathEffect="$pathEffect_bad_z13" />
        <case minzoom="14" maxzoom="14" pathEffect_0="$pathEffect_bad_z14" pathEffect="$pathEffect_bad_z14" />
        <case minzoom="15" maxzoom="15" pathEffect_0="$pathEffect_bad_z15" pathEffect="$pathEffect_bad_z15" />
        <case minzoom="16" maxzoom="16" pathEffect_0="$pathEffect_bad_z16" pathEffect="$pathEffect_bad_z16" />
        <case minzoom="17" maxzoom="17" pathEffect_0="$pathEffect_bad_z17" pathEffect="$pathEffect_bad_z17" />
        <case minzoom="18" maxzoom="18" pathEffect_0="$pathEffect_bad_z18" pathEffect="$pathEffect_bad_z18" />
        <case minzoom="19" maxzoom="19" pathEffect_0="$pathEffect_bad_z19" pathEffect="$pathEffect_bad_z19" />
        <case minzoom="20" maxzoom="20" pathEffect_0="$pathEffect_bad_z20" pathEffect="$pathEffect_bad_z20" />
        <case minzoom="21" pathEffect_0="$pathEffect_bad_z21" pathEffect="$pathEffect_bad_z21" />
    </renderingAttribute>

    <renderingAttribute name="pathEffect_horrible">
        <case maxzoom="12" pathEffect_0="$pathEffect_horrible_z12" pathEffect="$pathEffect_horrible_z12" />
        <case minzoom="13" maxzoom="13" pathEffect_0="$pathEffect_horrible_z13" pathEffect="$pathEffect_horrible_z13" />
        <case minzoom="14" maxzoom="14" pathEffect_0="$pathEffect_horrible_z14" pathEffect="$pathEffect_horrible_z14" />
        <case minzoom="15" maxzoom="15" pathEffect_0="$pathEffect_horrible_z15" pathEffect="$pathEffect_horrible_z15" />
        <case minzoom="16" maxzoom="16" pathEffect_0="$pathEffect_horrible_z16" pathEffect="$pathEffect_horrible_z16" />
        <case minzoom="17" maxzoom="17" pathEffect_0="$pathEffect_horrible_z17" pathEffect="$pathEffect_horrible_z17" />
        <case minzoom="18" maxzoom="18" pathEffect_0="$pathEffect_horrible_z18" pathEffect="$pathEffect_horrible_z18" />
        <case minzoom="19" maxzoom="19" pathEffect_0="$pathEffect_horrible_z19" pathEffect="$pathEffect_horrible_z19" />
        <case minzoom="20" maxzoom="20" pathEffect_0="$pathEffect_horrible_z20" pathEffect="$pathEffect_horrible_z20" />
        <case minzoom="21" pathEffect_0="$pathEffect_horrible_z21" pathEffect="$pathEffect_horrible_z21" />
    </renderingAttribute>

    <renderingAttribute name="pathEffect_no">
        <case maxzoom="12" pathEffect_0="$pathEffect_no_z12" pathEffect="$pathEffect_no_z12" />
        <case minzoom="13" maxzoom="13" pathEffect_0="$pathEffect_no_z13" pathEffect="$pathEffect_no_z13" />
        <case minzoom="14" maxzoom="14" pathEffect_0="$pathEffect_no_z14" pathEffect="$pathEffect_no_z14" />
        <case minzoom="15" maxzoom="15" pathEffect_0="$pathEffect_no_z15" pathEffect="$pathEffect_no_z15" />
        <case minzoom="16" maxzoom="16" pathEffect_0="$pathEffect_no_z16" pathEffect="$pathEffect_no_z16" />
        <case minzoom="17" maxzoom="17" pathEffect_0="$pathEffect_no_z17" pathEffect="$pathEffect_no_z17" />
        <case minzoom="18" maxzoom="18" pathEffect_0="$pathEffect_no_z18" pathEffect="$pathEffect_no_z18" />
        <case minzoom="19" maxzoom="19" pathEffect_0="$pathEffect_no_z19" pathEffect="$pathEffect_no_z19" />
        <case minzoom="20" maxzoom="20" pathEffect_0="$pathEffect_no_z20" pathEffect="$pathEffect_no_z20" />
        <case minzoom="21" pathEffect_0="$pathEffect_no_z21" pathEffect="$pathEffect_no_z21" />
    </renderingAttribute>

    <renderingAttribute name="pathEffect_markup">
        <case minzoom="15" maxzoom="15" pathEffect_2="4_32" pathEffect_3="0_4_4_28" pathEffect_4="0_8_4_24" pathEffect_5="0_12_4_20"/>
        <case minzoom="16" maxzoom="16" pathEffect_2="5_40" pathEffect_3="0_5_5_35" pathEffect_4="0_10_5_30" pathEffect_5="0_15_5_25"/>
        <case minzoom="17" maxzoom="17" pathEffect_2="6_48" pathEffect_3="0_6_6_42" pathEffect_4="0_12_6_36" pathEffect_5="0_18_6_30"/>
        <case minzoom="18" maxzoom="18" pathEffect_2="7_56" pathEffect_3="0_7_7_49" pathEffect_4="0_14_7_42" pathEffect_5="0_21_7_35"/>
        <case minzoom="19" maxzoom="19" pathEffect_2="8_64" pathEffect_3="0_8_8_56" pathEffect_4="0_16_8_48" pathEffect_5="0_24_8_40"/>
        <case minzoom="20" maxzoom="20" pathEffect_2="9_72" pathEffect_3="0_9_9_63" pathEffect_4="0_18_9_54" pathEffect_5="0_27_9_45"/>
        <case minzoom="21" pathEffect_2="10_80" pathEffect_3="0_10_10_70" pathEffect_4="0_20_10_60" pathEffect_5="0_30_10_50"/>
    </renderingAttribute>

    <renderingConstant name="c_track_outer" value="#925D3A"/>
    <renderingConstant name="c_track_inner" value="#B3A18F"/>

    <renderingConstant name="c_diff_0" value="#00A10F"/>
    <renderingConstant name="c_diff_1" value="#2C5DF2"/>
    <renderingConstant name="c_diff_2" value="#FF1111"/>
    <renderingConstant name="c_diff_3" value="#000000"/>
    <renderingConstant name="c_diff_4" value="#FF00FF"/>

    <order>
        <!-- 
        Backgrounds: 1-64: default 32
        Lines:
        Layer -2: 65-89: default 77
        Layer -1: 90-114: default 102
        Layer 0: 115-139: default 127
        Layer 1: 140-164: default 152
        Layer 2: 165-189: default 177
        Layer 3: 190-214: default 202
        Points: 215-255: default 240
        -->
        <switch>
            <switch area="false" point="false" objectType="2">
                <switch order="127">
                    <case tag="highway" value="track" minzoom="12"/>
                    <case tag="highway" value="path" minzoom="13"/>
                    <apply additional="layer=-2" order="77"/>
                    <apply layer="-1" order="102"/>
                    <apply layer="1" order="152"/>
                    <apply additional="layer=2" order="177"/>
                    <apply additional="layer=3" order="202"/>
                </switch>
            </switch>
        </switch>
    </order>
    <line>
        <switch>
            <case tag="highway" value="track" color="$c_track_inner" color_0="$c_track_outer">
                <switch>
                    <case additional="tracktype=grade1"/>
                    <case additional="tracktype=grade2" pathEffect_0="$pathEffect_good" pathEffect="$pathEffect_good"/>
                    <case additional="tracktype=grade3" pathEffect_0="$pathEffect_intermediate" pathEffect="$pathEffect_intermediate"/>
                    <case additional="tracktype=grade4" pathEffect_0="$pathEffect_bad" pathEffect="$pathEffect_bad"/>
                    <case additional="tracktype=grade5" pathEffect_0="$pathEffect_horrible" pathEffect="$pathEffect_horrible"/>
                    <case pathEffect_0="$pathEffect_unknown" pathEffect="$pathEffect_unknown" />
                </switch>
                <apply strokeWidth_0="$width_l2" />
            </case>
            <case tag="highway" value="path" color="$c_track_outer" cap="ROUND">
                <switch>
                    <case additional="visibility=excellent"/>
                    <case additional="visibility=good" pathEffect="$pathEffect_good"/>
                    <case additional="visibility=intermediate" pathEffect="$pathEffect_intermediate"/>
                    <case additional="visibility=bad" pathEffect="$pathEffect_bad"/>
                    <case additional="visibility=horrible" pathEffect="$pathEffect_horrible"/>
                    <case additional="visibility=no" pathEffect="$pathEffect_no" />
                    <case pathEffect="$pathEffect_unknown" />
                </switch>
            </case>
            <apply pathEffect_3="$pathEffect_markup">
                <switch>
                    <case additional="mtb:scale=0" color_3="$c_diff_0"/>
                    <case additional="mtb:scale=1" color_3="$c_diff_1"/>
                    <case additional="mtb:scale=2" color_3="$c_diff_2"/>
                    <case additional="mtb:scale=3" color_3="$c_diff_3"/>
                    <case additional="mtb:scale=4" color_3="$c_diff_4"/>
                    <case additional="mtb:scale=5" color_3="$c_diff_4"/>
                    <case additional="mtb:scale=6" color_3="$c_diff_4"/>
                    <apply color_2="#FFFFFF"/>
                </switch>
            </apply>
            <apply pathEffect_4="$pathEffect_markup">
                <switch>
                    <case additional="mtb:scale:uphill=0" color_4="$c_diff_0"/>
                    <case additional="mtb:scale:uphill=1" color_4="$c_diff_1"/>
                    <case additional="mtb:scale:uphill=2" color_4="$c_diff_2"/>
                    <case additional="mtb:scale:uphill=3" color_4="$c_diff_3"/>
                    <case additional="mtb:scale:uphill=4" color_4="$c_diff_4"/>
                    <case additional="mtb:scale:uphill=5" color_4="$c_diff_4"/>
                    <apply color_2="#FFFFFF"/>

                </switch>
            </apply>
            <apply pathEffect_5="$pathEffect_markup">
                <switch>
                    <case additional="sac_scale=hiking" color_5="$c_diff_0"/>
                    <case additional="sac_scale=mountain_hiking" color_5="$c_diff_1"/>
                    <case additional="sac_scale=demanding_mountain_hiking" color_5="$c_diff_2"/>
                    <case additional="sac_scale=alpine_hiking" color_5="$c_diff_3"/>
                    <case additional="sac_scale=demanding_alpine_hiking" color_5="$c_diff_4"/>
                    <case additional="sac_scale=difficult_alpine_hiking" color_5="$c_diff_4"/>
                    <apply color_2="#FFFFFF"/>

                </switch>
            </apply>
            <!-- width_l1 also sets width for markups-->
            <apply strokeWidth="$width_l1"/>
        </switch>
    </line>
</renderingStyle>